@model IEnumerable<DoWorkViewModel>
@foreach (var item in Model)
{
    <div class="col-md-6 m-15px-tb">
    <div class="feature-box-01 media">
        <i class="icon theme-bg @item.DoWorkIcon"></i>
        <div class="feature-content media-body">
            <h5 class="primary-font">@item.DoWorkTitle</h5>
            <p>
                @item.DoWorkDesc
            </p>
        </div>
    </div>
</div>
}
